package ex01;

public class InputOutput01 {

	public static void main(String[] args) {
		String myName = "이우빈";
		int myAge = 31;
		
				
		System.out.println("이름 : " + myName);
		System.out.println("나이 : " + myAge);

		System.out.println("나누기 : " + 5/3);
		System.out.println("나머지 : " + 5%3);

	}

}
