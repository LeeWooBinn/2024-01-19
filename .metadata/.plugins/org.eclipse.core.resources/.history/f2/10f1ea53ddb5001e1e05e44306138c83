package ex03;

public class Exan01 {

	public static void main(String[] args) {
		
		int time = 4000;
		int hour = time/(60*60);
		int minute = (time/60)%60;
		int second = time%60;
		System.out.println(time +"는 " + hour + "시간, " + minute + "분, " + second + "초.");

        int x = 1;
        
        int y = 2;
        
        int z = ++y + -y;
        
        System.out.println("출력결과 :" + z);
        
		
		}


}
